{
    "roots": [
      "<rootDir>/src/_domain/"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"]
}